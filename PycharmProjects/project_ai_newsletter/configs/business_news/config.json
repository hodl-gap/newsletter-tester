{
  "name": "business_news",
  "description": "AI business news aggregation - funding, M&A, product launches, partnerships",
  "default_max_age_hours": 24,
  "html_exclusions": [
    {"domain": "asiatechreview.com", "reason": "Domain inaccessible"},
    {"domain": "hai.stanford.edu", "reason": "Annual report, not news"},
    {"domain": "whitecase.com", "reason": "Law firm, not media"},
    {"domain": "foreignaffairsforum.ae", "reason": "Think tank, not news"},
    {"domain": "reuters.com", "reason": "DataDome protection"},
    {"domain": "wsj.com", "reason": "Paywalled"}
  ],
  "browser_use_sources": [
    {"url": "https://tech.economictimes.indiatimes.com/", "enabled": true, "name": "Economic Times"},
    {"url": "https://www.scmp.com/tech", "enabled": true, "name": "SCMP"},
    {"url": "https://www.cnbc.com/technology/", "enabled": true, "name": "CNBC"},
    {"url": "https://www.thenationalnews.com/business/technology/", "enabled": false, "name": "The National News"},
    {"url": "https://financeme.com/", "enabled": false, "name": "Finance ME"},
    {"url": "https://baobabnetwork.com/", "enabled": false, "name": "Baobab Network"}
  ],
  "browser_use_settings": {
    "headless": true,
    "max_articles_per_source": 20,
    "model": "claude-sonnet-4-20250514"
  },
  "valid_regions": [
    "north_america", "latin_america", "europe", "middle_east",
    "africa", "south_asia", "southeast_asia", "east_asia",
    "oceania", "global", "unknown"
  ],
  "valid_categories": [
    "funding", "acquisition", "product_launch", "partnership",
    "earnings", "expansion", "executive", "ipo",
    "regulation", "strategy", "research"
  ],
  "valid_layers": [
    "chips_infra", "foundation_models", "finetuning_mlops",
    "b2b_apps", "consumer_apps"
  ]
}
